{
  "name": "YouTube Short Redirect Extension",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Redirects YouTube short URLs to normal video format.",
  "permissions": ["tabs"],
  "icons": {
    "16": "icons/icon16x16.png",
    "48": "icons/icon48x48.png",
    "128": "icons/icon128x128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
